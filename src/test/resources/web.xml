  version="3.0">

<web-app>

  <display-name>Archetype Created Web Application</display-name>
  
  <servlet>
    <servlet-name>TestServlet</servlet-name>
    <servlet-class>de.chkal.arquillian.test.TestServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>TestServlet</servlet-name>
    <url-pattern>/*</url-pattern>
  </servlet-mapping>
  
</web-app>
